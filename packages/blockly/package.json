{
  "name": "jupyterlab-broccoli",
  "version": "0.4.0",
  "description": "Fork of blockly extension for JupyterLab.",
  "keywords": [
    "blockly",
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/FumiHax/jupyterlab-broccoli",
  "bugs": {
    "url": "https://github.com/FumiHax/jupyterlab-broccoli/issues"
  },
  "license": "BSD-3-Clause",
  "author": {
    "name": "Fumi.Iseki",
    "email": "iseki@rsch.tuis.ac.jp"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,js,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/FumiHax/jupyterlab-broccoli.git"
  },
  "scripts": {
    "build": "tsc -b",
    "build:prod": "jlpm build",
    "clean": "jlpm clean:lib",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "clean:all": "jlpm clean:lib",
    "install:extension": "jlpm build"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^4.0",
    "@jupyterlab/cells": "^4.0",
    "@jupyterlab/codeeditor": "^4.0",
    "@jupyterlab/coreutils": "^5.6",
    "@jupyterlab/docregistry": "^4.0",
    "@jupyterlab/rendermime": "^4.0",
    "@jupyterlab/services": "^7.0",
    "@jupyterlab/ui-components": "^4.0",
    "@lumino/algorithm": "^1.9.1",
    "@lumino/coreutils": "^1.12.0",
    "@lumino/messaging": "^1.10.1",
    "@lumino/signaling": "^1.10.1",
    "@lumino/widgets": "^2.0",
    "@types/react": "^18.0.26",
    "blockly": "^9.2.1"
  },
  "devDependencies": {
    "rimraf": "^4.4.0",
    "typescript": "~4.9.5"
  },
  "sideEffects": [
    "style/*.css",
    "style/index.js"
  ],
  "styleModule": "style/index.js",
  "publishConfig": {
    "access": "public"
  }
}
